{
  "name": "super-auth",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/super-auth/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    }
  },
  "build": {
  "executor": "@nx/rollup:rollup",
  "outputs": ["{options.outputPath}"],
  "options": {
    "outputPath": "dist/packages/super-auth",
    "entryFile": "packages/super-auth/src/index.ts",
    "tsConfig": "packages/super-auth/tsconfig.lib.json",
    "project": "packages/super-auth/project.json",
    "external": ["tslib"],
    "rollupConfig": "packages/super-auth/rollup.config.cjs"
  }
}

}
